    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="/fonts/pe-icon-7-stroke.css" rel="stylesheet" />

</head>
<body>

<div class="wrapper">
    <div class="sidebar" data-color="purple" data-image="assets/img/sidebar-5.jpg">

    <!--

        Tip 1: you can change the color of the sidebar using: data-color="blue | azure | green | orange | red | purple"
        Tip 2: you can also add an image using data-image tag

    -->

    	<div class="sidebar-wrapper">
            <div class="logo">
                <a href="http://cet.edu." class="simple-text">
                    QUIZ APP
                </a>
            </div>

            <ul class="nav">
                <li class="active">
                    <a href="#">
                        <i class="pe-7s-graph"></i>
                        <p>Dashboard</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="pe-7s-user"></i>
                        <p>User Profile</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <i class="pe-7s-note2"></i>
                        <p>Scores</p>
                    </a>
                </li>
    	</div>
    </div>

    <div class="main-panel">
        <nav class="navbar navbar-default navbar-fixed">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="#"><%= users.name%></a>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a href="/users/logout">
                                <p>Log out</p>
                            </a>
                        </li>
						<li class="separator hidden-lg hidden-md"></li>
                    </ul>
                </div>
            </div>
        </nav>


        <div class="content">
            <div class="container-fluid">
                <div class="row">
									<a href="/users/dashboard4"><div class="col-md-8">
											<div class="card ">
													<div class="header">
															<h4 class="title">SCIENCE</h4>
															<p class="category">Nothing is undefined</p>
													</div>
													<div class="content">
														<img src="/images/science.jpg" alt="science" style="width:100%" />
													</div>
											</div>
									</div></a>
                  <a href="/users/dashboard3"><div class="col-md-4">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">INFORMATION TECHNOLOGY</h4>
                                <p class="category">The world wants to know</p>
                            </div>
                            <div class="content">
                                <img src="/images/it.jpg" alt="IT image" style="width:100%"/>
                            </div>
                        </div>
                    </div></a>
                </div>
								<div class="row">
									<a href="/users/dashboard2"><div class="col-md-4">
											<div class="card">
													<div class="header">
															<h4 class="title">SPORTS</h4>
															<p class="category">The area where everyone has something to answer</p>
													</div>

													<div class="content">
															<img src="/images/sports.jpg" alt="Sports quiz" style="width:100%"/>
															<!-- <div id="chartPreferences" class="ct-chart ct-perfect-fourth"></div> -->
													</div>
											</div>
									</div></a>
                </div>
            </div>
        </div>


        <footer class="footer">
            <div class="container-fluid">
                <p class="copyright pull-right">
                    &copy; <script>document.write(new Date().getFullYear())</script> CSE 2nd Year
                </p>
            </div>
        </footer>

    </div>
</div>


